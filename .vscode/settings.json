{
  "files.eol": "\n",
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "rvest.vs-code-prettier-eslint",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "vue",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "[svelte]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  },
  "html.customData": [
    "./node_modules/@shoelace-style/shoelace/dist/custom-elements.json"
  ]
}
